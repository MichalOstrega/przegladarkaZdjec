<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="500.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="przegladarka.Controller">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem fx:id="openItem" mnemonicParsing="false" text="Open…" />
                        <MenuItem fx:id="openFolder" mnemonicParsing="false" text="Open Folder" />
                        <MenuItem fx:id="closeitem" mnemonicParsing="false" text="Close" />
                        <MenuItem fx:id="saveItem" mnemonicParsing="false" text="Save As…" />
                        <MenuItem fx:id="quitItem" mnemonicParsing="false" text="Quit" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
      <VBox alignment="CENTER">
         <children>
      
                      <ImageView fx:id="image" fitHeight="425.0" fitWidth="640.0" pickOnBounds="true" preserveRatio="true" />
            <TextField alignment="CENTER" text="ALE EKSTRA ZDJĘCIE!" />
         </children>
      </VBox>
            
        <HBox prefHeight="50.0" prefWidth="640.0">
            <children>
                <Button fx:id="prevButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Prev">
                    <HBox.margin>
                        <Insets bottom="10.0" left="110.0" right="110.0" top="10.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="nextButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Next">
                    <HBox.margin>
                        <Insets bottom="10.0" left="110.0" right="110.0" top="10.0" />
                    </HBox.margin>
                </Button>
            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </HBox>
    </children>
</VBox>
